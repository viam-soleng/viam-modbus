{
  "module_id": "viam-soleng:viam-modbus",
  "visibility": "public",
  "url": "https://github.com/viam-soleng/viam-modbus",
  "description": "A module to read and write data from Modbus devices",
  "models": [
    {
      "api": "rdk:component:board",
      "model": "viam-soleng:modbus:client_tcp"
    },
    {
      "api":"rdk:component:sensor",
      "model":"viam-soleng:modbus:client_tcp"
    },
    {
      "api": "rdk:component:sensor",
      "model": "viam-soleng:modbus:client_ascii"
    },
    {
      "api": "rdk:component:board",
      "model": "viam-soleng:modbus:client_ascii"
    },
    {
      "api": "rdk:component:sensor",
      "model": "viam-soleng:modbus:client_rtu"
    },
    {
      "api": "rdk:component:board",
      "model": "viam-soleng:modbus:client_rtu"
    }
  ],
  "build": {
    "setup": "./setup.sh",
    "build": "make package",
    "path": "viam-modbus.tgz",
    "arch": ["linux/amd64", "linux/arm64", "darwin/arm64"]
  },
  "entrypoint": "bin/viam-modbus"
}
